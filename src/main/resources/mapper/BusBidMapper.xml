    left join bus_account_notification ban
    on bb.AccountCode = ban.AccountCode
    where ban.BiddingNewHighestBid in ('1','3')
    and bb.CommondityCode = #{commodityCode}
    and bb.SizeName= #{sizeName}
  </select>

  <select id="getBidExpiring" resultMap="BaseResultMap" useCache="true">
    select * from bus_bid where date_add(BidTime, interval BidExpiration-1 day) &lt;= now()
  </select>

  <select id="getBidExpired" resultMap="BaseResultMap" useCache="true">
    select * from bus_bid where date_add(BidTime, interval BidExpiration day) &lt; now()
  </select>

  <select id="getAskMatchesExpiredBid" resultMap="BaseResultMap" useCache="true">
    select bb.* from bus_bid_history bb
    left join bus_account_notification ban
    on bb.AccountCode = ban.AccountCode
    where ban.AskMatchesExpiredBid in ('1','3')
    and bb.CommondityCode = #{commodityCode}
    and bb.SizeName = #{sizeName}
    and bb.BidPrice = #{askPrice}
  </select>

  <select id="getBuyerNearbyMatch" resultMap="BaseResultMap" useCache="true">
    select bb.* from bus_bid bb
    left join bus_account_notification ban
    on bb.AccountCode = ban.AccountCode
    where ban.BuyerNearbyMatch in ('1','3')
    and bb.CommondityCode = #{commodityCode} and bb.SizeName = #{sizeName}
    and bb.BidPrice &gt; #{askPrice}/2 and bb.BidPrice &lt;= #{askPrice}
  </select>
</mapper>