  <select id="getAskingNewHighestBid" resultMap="BaseResultMap" useCache="true">
    select ba.* from bus_ask ba
    left join bus_account_notification ban on ba.AccountCode = ban.AccountCode
    where ban.AskingNewHighestBid in ('1','3')
    and ba.CommondityCode = #{commodityCode}
    and ba.SizeName= #{sizeName}
    and #{bidPrice} &gt;= ba.AskPrice*ban.AskingNewHighestBidPercent
  </select>

  <select id="getAskingNewLowestAsk" resultMap="BaseResultMap" useCache="true">
    select ba.* from bus_ask ba
    left join bus_account_notification ban on ba.AccountCode = ban.AccountCode
    where ban.AskingNewLowestAsk in ('1','3')
    and ba.CommondityCode = #{commodityCode}
    and ba.SizeName= #{sizeName}
  </select>

  <select id="getAskExpiring" resultMap="BaseResultMap" useCache="true">
    select * from bus_ask where date_add(AskTime, interval AskExpiration-1 day) &lt;= now()
  </select>

  <select id="getAskExpired" resultMap="BaseResultMap" useCache="true">
    select * from bus_ask where date_add(AskTime, interval AskExpiration day) &lt; now()
  </select>
</mapper>